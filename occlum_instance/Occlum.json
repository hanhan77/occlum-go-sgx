{
    "resource_limits": {
        "user_space_size": "256MB",
        "kernel_space_heap_size": "16MB",
        "kernel_space_stack_size": "1MB",
        "max_num_of_threads": 16
    },
    "process": {
        "default_stack_size": "4MB",
        "default_heap_size": "16MB",
        "default_mmap_size": "64MB"
    },
    "entry_points": [
        "/bin/app"
    ],
    "env": {
        "default": [
            "OCCLUM=yes",
            "OCCLUM_DISABLE_FSGSBASE=1"
        ]
    },
    "mount": [
        {
            "target": "/bin",
            "type": "unionfs",
            "options": {
                "layers": [
                    {
                        "target": "/bin",
                        "type": "sefs",
                        "source": "./image/bin"
                    }
                ]
            }
        },
        {
            "target": "/lib",
            "type": "unionfs",
            "options": {
                "layers": [
                    {
                        "target": "/lib",
                        "type": "sefs",
                        "source": "./image/lib"
                    }
                ]
            }
        },
        {
            "target": "/data",
            "type": "unionfs",
            "options": {
                "layers": [
                    {
                        "target": "/data",
                        "type": "encrypted",
                        "source": "image/data"
                    }
                ]
            }
        }
    ],
    "security": {
        "enable_fsgsbase": false,
        "enable_kss": false,
        "enable_kss_verify": false,
        "enable_kss_verify_remote": false
    }
}
